<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI" 
                  id="Definitions_Simple" 
                  targetNamespace="http://bpmn.io/schema/bpmn">

  <bpmn:collaboration id="Collaboration_MerchantIntegrationSimple">
    
    <!-- Sales Engineer Pool -->
    <bpmn:participant id="Participant_SE_Simple" name="Sales Engineer" processRef="Process_SE_Simple" />
    
    <!-- Implementation Engineer Pool -->
    <bpmn:participant id="Participant_IE_Simple" name="Implementation Engineer" processRef="Process_IE_Simple" />
    
    <!-- Merchant Pool -->
    <bpmn:participant id="Participant_Merchant_Simple" name="Merchant" processRef="Process_Merchant_Simple" />
    
    <!-- Message Flows -->
    <bpmn:messageFlow id="MessageFlow_Handover_Simple" sourceRef="Task_SE_Handover" targetRef="Task_IE_KickoffMeeting" />
    <bpmn:messageFlow id="MessageFlow_ScopeValidation_Simple" sourceRef="Task_IE_KickoffMeeting" targetRef="Task_Merchant_KickoffParticipation" />
    <bpmn:messageFlow id="MessageFlow_IntegrationSupport_Simple" sourceRef="Task_IE_SupportIntegration" targetRef="Task_Merchant_Integration" />
    <bpmn:messageFlow id="MessageFlow_Certification_Simple" sourceRef="Task_IE_Certification" targetRef="Gateway_Merchant_CertificationResult" />
    <bpmn:messageFlow id="MessageFlow_GoLive_Simple" sourceRef="Task_Merchant_GoLive" targetRef="Task_IE_Monitor" />
    
  </bpmn:collaboration>

  <!-- Sales Engineer Process -->
  <bpmn:process id="Process_SE_Simple" isExecutable="false">
    <bpmn:task id="Task_SE_Handover" name="Schedule handover with IE, send merchant scope" />
  </bpmn:process>

  <!-- Implementation Engineer Process -->
  <bpmn:process id="Process_IE_Simple" isExecutable="false">
    <bpmn:startEvent id="StartEvent_Process" name="Integration Process Starts" />
    <bpmn:task id="Task_IE_KickoffMeeting" name="Schedule and conduct kickoff meeting - validate scope" />
    <bpmn:exclusiveGateway id="Gateway_IE_ScopeValid" name="Is scope valid?" />
    <bpmn:task id="Task_IE_SupportIntegration" name="Support merchant integration (IE handles integration)" />
    <bpmn:task id="Task_IE_Certification" name="Certify the integration" />
    <bpmn:exclusiveGateway id="Gateway_IE_CertificationResult" name="Is integration correct?" />
    <bpmn:task id="Task_IE_Monitor" name="Monitor for 30 days" />
    <bpmn:task id="Task_IE_HandoverTAM" name="Handover to TAM team" />
    <bpmn:endEvent id="EndEvent_Process" name="Process Complete" />
    
    <bpmn:sequenceFlow id="Flow_Start_Kickoff" sourceRef="StartEvent_Process" targetRef="Task_IE_KickoffMeeting" />
    <bpmn:sequenceFlow id="Flow_Kickoff_Gateway" sourceRef="Task_IE_KickoffMeeting" targetRef="Gateway_IE_ScopeValid" />
    <bpmn:sequenceFlow id="Flow_Valid_Integration" name="Yes" sourceRef="Gateway_IE_ScopeValid" targetRef="Task_IE_SupportIntegration" />
    <bpmn:sequenceFlow id="Flow_Invalid_Handover" name="No" sourceRef="Gateway_IE_ScopeValid" targetRef="Task_SE_Handover" />
    <bpmn:sequenceFlow id="Flow_Integration_Certification" sourceRef="Task_IE_SupportIntegration" targetRef="Task_IE_Certification" />
    <bpmn:sequenceFlow id="Flow_Certification_Gateway" sourceRef="Task_IE_Certification" targetRef="Gateway_IE_CertificationResult" />
    <bpmn:sequenceFlow id="Flow_Certified_Monitor" name="Yes" sourceRef="Gateway_IE_CertificationResult" targetRef="Task_IE_Monitor" />
    <bpmn:sequenceFlow id="Flow_NotCertified_Retry" name="No" sourceRef="Gateway_IE_CertificationResult" targetRef="Task_IE_Certification" />
    <bpmn:sequenceFlow id="Flow_Monitor_TAM" sourceRef="Task_IE_Monitor" targetRef="Task_IE_HandoverTAM" />
    <bpmn:sequenceFlow id="Flow_TAM_End" sourceRef="Task_IE_HandoverTAM" targetRef="EndEvent_Process" />
  </bpmn:process>

  <!-- Merchant Process -->
  <bpmn:process id="Process_Merchant_Simple" isExecutable="false">
    <bpmn:task id="Task_Merchant_KickoffParticipation" name="Participate in kickoff meeting" />
    <bpmn:task id="Task_Merchant_Integration" name="Start integration with IE support" />
    <bpmn:exclusiveGateway id="Gateway_Merchant_CertificationResult" name="Certification result" />
    <bpmn:task id="Task_Merchant_Adjustments" name="Adjust integration" />
    <bpmn:task id="Task_Merchant_GoLive" name="Go live" />
    
    <bpmn:sequenceFlow id="Flow_Merchant_Kickoff_Integration" sourceRef="Task_Merchant_KickoffParticipation" targetRef="Task_Merchant_Integration" />
    <bpmn:sequenceFlow id="Flow_Merchant_Integration_Gateway" sourceRef="Task_Merchant_Integration" targetRef="Gateway_Merchant_CertificationResult" />
    <bpmn:sequenceFlow id="Flow_Merchant_Adjustments" name="Needs adjustment" sourceRef="Gateway_Merchant_CertificationResult" targetRef="Task_Merchant_Adjustments" />
    <bpmn:sequenceFlow id="Flow_Merchant_Retry" sourceRef="Task_Merchant_Adjustments" targetRef="Task_IE_Certification" />
    <bpmn:sequenceFlow id="Flow_Merchant_GoLive" name="Approved" sourceRef="Gateway_Merchant_CertificationResult" targetRef="Task_Merchant_GoLive" />
  </bpmn:process>

  <!-- Diagram Interchange -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_Simple">
    <bpmndi:BPMNPlane id="BPMNPlane_Simple" bpmnElement="Collaboration_MerchantIntegrationSimple">
      
      <!-- Sales Engineer Pool -->
      <bpmndi:BPMNShape id="Participant_SE_Simple_di" bpmnElement="Participant_SE_Simple" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="1000" height="120" />
      </bpmndi:BPMNShape>
      
      <!-- Implementation Engineer Pool -->
      <bpmndi:BPMNShape id="Participant_IE_Simple_di" bpmnElement="Participant_IE_Simple" isHorizontal="true">
        <dc:Bounds x="160" y="220" width="1000" height="200" />
      </bpmndi:BPMNShape>
      
      <!-- Merchant Pool -->
      <bpmndi:BPMNShape id="Participant_Merchant_Simple_di" bpmnElement="Participant_Merchant_Simple" isHorizontal="true">
        <dc:Bounds x="160" y="440" width="1000" height="160" />
      </bpmndi:BPMNShape>
      
      <!-- Sales Engineer Process Elements -->
      <bpmndi:BPMNShape id="Task_SE_Handover_di" bpmnElement="Task_SE_Handover">
        <dc:Bounds x="300" y="100" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Implementation Engineer Process Elements -->
      <bpmndi:BPMNShape id="StartEvent_Process_di" bpmnElement="StartEvent_Process">
        <dc:Bounds x="212" y="272" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="190" y="315" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_IE_KickoffMeeting_di" bpmnElement="Task_IE_KickoffMeeting">
        <dc:Bounds x="300" y="250" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_IE_ScopeValid_di" bpmnElement="Gateway_IE_ScopeValid" isMarkerVisible="true">
        <dc:Bounds x="450" y="265" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="437" y="322" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_IE_SupportIntegration_di" bpmnElement="Task_IE_SupportIntegration">
        <dc:Bounds x="550" y="250" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_IE_Certification_di" bpmnElement="Task_IE_Certification">
        <dc:Bounds x="700" y="250" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_IE_CertificationResult_di" bpmnElement="Gateway_IE_CertificationResult" isMarkerVisible="true">
        <dc:Bounds x="850" y="265" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="840" y="322" width="71" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_IE_Monitor_di" bpmnElement="Task_IE_Monitor">
        <dc:Bounds x="950" y="250" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_IE_HandoverTAM_di" bpmnElement="Task_IE_HandoverTAM">
        <dc:Bounds x="1100" y="250" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="EndEvent_Process_di" bpmnElement="EndEvent_Process">
        <dc:Bounds x="1252" y="272" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1227" y="315" width="86" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Merchant Process Elements -->
      <bpmndi:BPMNShape id="Task_Merchant_KickoffParticipation_di" bpmnElement="Task_Merchant_KickoffParticipation">
        <dc:Bounds x="300" y="460" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Merchant_Integration_di" bpmnElement="Task_Merchant_Integration">
        <dc:Bounds x="550" y="460" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_Merchant_CertificationResult_di" bpmnElement="Gateway_Merchant_CertificationResult" isMarkerVisible="true">
        <dc:Bounds x="725" y="475" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="711" y="532" width="78" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Merchant_Adjustments_di" bpmnElement="Task_Merchant_Adjustments">
        <dc:Bounds x="600" y="550" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_Merchant_GoLive_di" bpmnElement="Task_Merchant_GoLive">
        <dc:Bounds x="950" y="460" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Sequence Flows -->
      <bpmndi:BPMNEdge id="Flow_Start_Kickoff_di" bpmnElement="Flow_Start_Kickoff">
        <di:waypoint x="248" y="290" />
        <di:waypoint x="300" y="290" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Kickoff_Gateway_di" bpmnElement="Flow_Kickoff_Gateway">
        <di:waypoint x="400" y="290" />
        <di:waypoint x="450" y="290" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Valid_Integration_di" bpmnElement="Flow_Valid_Integration">
        <di:waypoint x="500" y="290" />
        <di:waypoint x="550" y="290" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="517" y="272" width="18" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Invalid_Handover_di" bpmnElement="Flow_Invalid_Handover">
        <di:waypoint x="475" y="265" />
        <di:waypoint x="475" y="140" />
        <di:waypoint x="400" y="140" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="484" y="200" width="15" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Integration_Certification_di" bpmnElement="Flow_Integration_Certification">
        <di:waypoint x="650" y="290" />
        <di:waypoint x="700" y="290" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Certification_Gateway_di" bpmnElement="Flow_Certification_Gateway">
        <di:waypoint x="800" y="290" />
        <di:waypoint x="850" y="290" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Certified_Monitor_di" bpmnElement="Flow_Certified_Monitor">
        <di:waypoint x="900" y="290" />
        <di:waypoint x="950" y="290" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="917" y="272" width="18" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_NotCertified_Retry_di" bpmnElement="Flow_NotCertified_Retry">
        <di:waypoint x="875" y="315" />
        <di:waypoint x="875" y="360" />
        <di:waypoint x="750" y="360" />
        <di:waypoint x="750" y="330" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="884" y="342" width="15" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Monitor_TAM_di" bpmnElement="Flow_Monitor_TAM">
        <di:waypoint x="1050" y="290" />
        <di:waypoint x="1100" y="290" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_TAM_End_di" bpmnElement="Flow_TAM_End">
        <di:waypoint x="1200" y="290" />
        <di:waypoint x="1252" y="290" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Merchant_Kickoff_Integration_di" bpmnElement="Flow_Merchant_Kickoff_Integration">
        <di:waypoint x="400" y="500" />
        <di:waypoint x="550" y="500" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Merchant_Integration_Gateway_di" bpmnElement="Flow_Merchant_Integration_Gateway">
        <di:waypoint x="650" y="500" />
        <di:waypoint x="725" y="500" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Merchant_Adjustments_di" bpmnElement="Flow_Merchant_Adjustments">
        <di:waypoint x="750" y="525" />
        <di:waypoint x="750" y="590" />
        <di:waypoint x="700" y="590" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="759" y="555" width="82" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Merchant_Retry_di" bpmnElement="Flow_Merchant_Retry">
        <di:waypoint x="600" y="590" />
        <di:waypoint x="750" y="590" />
        <di:waypoint x="750" y="330" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Merchant_GoLive_di" bpmnElement="Flow_Merchant_GoLive">
        <di:waypoint x="775" y="500" />
        <di:waypoint x="950" y="500" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="844" y="482" width="48" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <!-- Message Flows -->
      <bpmndi:BPMNEdge id="MessageFlow_Handover_Simple_di" bpmnElement="MessageFlow_Handover_Simple">
        <di:waypoint x="350" y="180" />
        <di:waypoint x="350" y="250" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="MessageFlow_ScopeValidation_Simple_di" bpmnElement="MessageFlow_ScopeValidation_Simple">
        <di:waypoint x="350" y="330" />
        <di:waypoint x="350" y="460" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="MessageFlow_IntegrationSupport_Simple_di" bpmnElement="MessageFlow_IntegrationSupport_Simple">
        <di:waypoint x="600" y="330" />
        <di:waypoint x="600" y="460" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="MessageFlow_Certification_Simple_di" bpmnElement="MessageFlow_Certification_Simple">
        <di:waypoint x="750" y="330" />
        <di:waypoint x="750" y="475" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="MessageFlow_GoLive_Simple_di" bpmnElement="MessageFlow_GoLive_Simple">
        <di:waypoint x="1000" y="460" />
        <di:waypoint x="1000" y="330" />
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions> 